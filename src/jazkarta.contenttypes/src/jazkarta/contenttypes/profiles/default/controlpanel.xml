<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="jazkarta.contenttypes"
    purge="False">

    <configlet
        title="Jazkarta Content Type Settings"
        action_id="jazkarta_contenttypes"
        appId="jazkarta.contenttypes"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@jaz-contenttypes-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
